          "type": "Link",
          "config": {
            "label": "Grid Section",
            "select": "document",
            "customtypes": ["section"]
          }
        },
		"column_count": {
		  "type": "Select",
		  "config": {
			"label": "Columns",
			"placeholder": "",
			"options": ["1", "2", "3", "4", "5"],
			"default_value": "1"
		  }
		},
		"column_gap": {
		  "type": "Boolean",
		  "config": {
			"label": "Column Gap",
			"placeholder_false": "no",
			"placeholder_true": "yes",
			"default_value": true
		  }
		},
		"column_alignment": {
		  "type": "Select",
		  "config": {
			"label": "Column Alignment",
			"placeholder": "",
			"options": ["Fill", "Top", "Center", "Bottom"],
			"default_value": "Fill"
		  }
		},
		"column_screen": {
		  "type": "Boolean",
		  "config": {
			"label": "Fullscreen (minimum)",
			"placeholder_false": "no",
			"placeholder_true": "yes",
			"default_value": false
		  }
		},
		"column_max_width": {
		  "type": "Number",
		  "config": {
			"label": "Column Max Width (px)",
