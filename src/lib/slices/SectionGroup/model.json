		  "type": "Select",
		  "config": {
			"label": "Section Padding",
			"placeholder": "",
			"options": [
			  "None",
			  "Top",
			  "Top + Bottom",
			  "Bottom",
			  "Left",
			  "Left + Right",
			  "Right",
			  "No Top",
			  "No Bottom",
			  "All"
			],
			"default_value": "None"
		  }
		},
		"section_background_color": {
		  "type": "Color",
		  "config": { "label": "Section Background Color", "placeholder": "" }
		}
